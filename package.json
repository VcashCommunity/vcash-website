{
  "name": "vcash-website",
  "productName": "Vcash community website",
  "version": "0.1.17",
  "description": "Vcash community website built using Next.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "standard",
    "format": "prettier-standard '{pages,src}/**/*.js' && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whphhg/vcash-website.git"
  },
  "keywords": [
    "vcash",
    "community",
    "website"
  ],
  "author": "whphhg <whphhg@protonmail.ch>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/whphhg/vcash-website/issues"
  },
  "homepage": "https://github.com/whphhg/vcash-website#readme",
  "standard": {
    "parser": "babel-eslint"
  },
  "wwwHost": {
    "client": "http://localhost:3000",
    "server": "http://localhost:3000",
    "releases": {
      "daemon": {
        "sha256sums": "https://github.com/openvcash/vcash/releases/download/0.6.0.4/vcash-0.6.0.4-sha256.asc",
